@model DIRECTION_GESTOR.Models.Actanacimiento

@{
    ViewData["Title"] = "Details";
}
<br><br>
<h2>Detalles</h2>

<div>
    <h4>Acta de Nacimiento</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Folio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FolioActaNacimiento)
        </dd>
        <dt>
            Año
        </dt>
        <dd>
            @Html.DisplayFor(model => model.AnioActaNacimiento)
        </dd>
        <dt>
            Libro
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LibroActaNacimiento)
        </dd>
        <dt>
            Numero
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NumeroActaNacimiento)
        </dd>
        <dt>
            Circunscripcion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdCircunscripcionFkNavigation.DescripcionCircunscripcion)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt style="display:none">
            Intitucion
        </dt>
        <dd style="display:none">
            @Html.DisplayFor(model => model.IdInstitucionFkNavigation.NombreInstitucion)
        </dd>
        <dt>
            Persona
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPersonaFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.IdPersonaFkNavigation.ApellidoPersonas)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdActaNacimiento" class="btn btn-primary">Editar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
