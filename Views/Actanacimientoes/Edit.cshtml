@model DIRECTION_GESTOR.Models.Actanacimiento

@{
    ViewData["Title"] = "Edit";
}
<br><br>
<h2>Editar Actanacimiento</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdActaNacimiento" />
            <div class="form-group">
                <label asp-for="FolioActaNacimiento" class="control-label">Folio</label>
                <input asp-for="FolioActaNacimiento" class="form-control" />
                <span asp-validation-for="FolioActaNacimiento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AnioActaNacimiento" class="control-label">Año</label>
                <input asp-for="AnioActaNacimiento" class="form-control" />
                <span asp-validation-for="AnioActaNacimiento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LibroActaNacimiento" class="control-label">Libro</label>
                <input asp-for="LibroActaNacimiento" class="form-control" />
                <span asp-validation-for="LibroActaNacimiento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NumeroActaNacimiento" class="control-label">Numero</label>
                <input asp-for="NumeroActaNacimiento" class="form-control" />
                <span asp-validation-for="NumeroActaNacimiento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdPersonaFk" class="control-label">Persona</label>
                <select asp-for="IdPersonaFk" class="form-control" asp-items="ViewBag.IdPersonaFk"></select>
                <span asp-validation-for="IdPersonaFk" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdEstadoFk" class="control-label">Estado</label>
                <select asp-for="IdEstadoFk" class="form-control" asp-items="ViewBag.IdEstadoFk"></select>
                <span asp-validation-for="IdEstadoFk" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdCircunscripcionFk" class="control-label">Circunscripcion</label>
                <select asp-for="IdCircunscripcionFk" class="form-control" asp-items="ViewBag.IdCircunscripcionFk"></select>
                <span asp-validation-for="IdCircunscripcionFk" class="text-danger"></span>
            </div>
            <div class="form-group" style="display:none">
                <label asp-for="IdInstitucionFk" class="control-label">Institucion</label>
                <select asp-for="IdInstitucionFk" class="form-control" asp-items="ViewBag.IdInstitucionFk"></select>
                <span asp-validation-for="IdInstitucionFk" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Modificar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver Atras</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
