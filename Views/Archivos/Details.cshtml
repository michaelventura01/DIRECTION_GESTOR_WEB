@model DIRECTION_GESTOR.Models.Archivos

@{
    ViewData["Title"] = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Archivos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Enlace de Archivo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EnlaceArchivo)
        </dd>
        <dt>
            Fecha de Ingreso
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaArchivo)
        </dd>
        <dt>
            Hora de Ingreso
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Hora)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt>
            Persona
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPersonaFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.IdPersonaFkNavigation.ApellidoPersonas)
        </dd>
        <dt>
            Tipo de Archivo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdTipoArchivoFkNavigation.DescripcionTipoArchivo)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdArchivo" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
