@model DIRECTION_GESTOR.Models.Asignaturasempleadosestudiantes

@{
    ViewData["Title"] = "Delete";
}
<br><br>
<h2>Eliminar</h2>
<hr>
<h3>Estas Seguro que deseas eliminar?</h3>
<div>
    <h4>Estudiantes Asignados a Asignaturas</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Estudiante
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CodigoEstudianteFkNavigation.IdPersonaFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.CodigoEstudianteFkNavigation.IdPersonaFkNavigation.ApellidoPersonas)
        </dd>
        <dt>
            Profesor
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdAsignaturaEmpleadoFkNavigation.CodigoEmpleadoFkNavigation.IdPersonaFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.IdAsignaturaEmpleadoFkNavigation.CodigoEmpleadoFkNavigation.IdPersonaFkNavigation.ApellidoPersonas)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt>
            Periodo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPeriodoFkNavigation.DescripcionPeriodo)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdAsignaturaEmpleadoEstudiante" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
