@model DIRECTION_GESTOR.Models.Aulas

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div>
    <h4>Aulas</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Aula
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionAula)
        </dd>
        <dt>
            Edificio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEdificioFkNavigation.DescripcionEdificio)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt style="display:none">
            Institucion
        </dt>
        <dd style="display:none">
            @Html.DisplayFor(model => model.IdInstitucionFkNavigation.NombreInstitucion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdAula" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
