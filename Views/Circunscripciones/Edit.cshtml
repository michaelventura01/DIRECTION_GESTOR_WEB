@model DIRECTION_GESTOR.Models.Circunscripciones

@{
    ViewData["Title"] = "Edit";
}
<BR><BR>
<h2>Editar Circunscripcion</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdCircunscripcion" />
            <div class="form-group">
                <label asp-for="DescripcionCircunscripcion" class="control-label">Circunscripcion</label>
                <input asp-for="DescripcionCircunscripcion" class="form-control" />
                <span asp-validation-for="DescripcionCircunscripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdEstadoFk" class="control-label">Estado</label>
                <select asp-for="IdEstadoFk" class="form-control" asp-items="ViewBag.IdEstadoFk"></select>
                <span asp-validation-for="IdEstadoFk" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Modificar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver Atras</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
