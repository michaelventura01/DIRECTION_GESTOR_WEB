@model DIRECTION_GESTOR.Models.Cursosasignaturas

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>Deseas Eliminar Esto?</h3>
<div>
    <h4>Asignaturas de Cursos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Asignatura
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdAsignaturaFkNavigation.DescripcionAsignatura)
        </dd>
        <dt>
            Curso
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdCursoFkNavigation.DescripcionCurso)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdCursoAsignatura" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
