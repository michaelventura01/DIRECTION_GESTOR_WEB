@model DIRECTION_GESTOR.Models.Edificios

@{
    ViewData["Title"] = "Details";
}
<br><br>
<h2>Detalles</h2>
<br>
<div>
    <h4>Edificio</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Edificio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionEdificio)
        </dd>
        <dt>
            Direccion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdDireccionFkNavigation.DescripcionDireccion), @Html.DisplayFor(model => model.IdDireccionFkNavigation.IdBarrioFkNavigation.DescripcionBarrio), @Html.DisplayFor(model => model.IdDireccionFkNavigation.IdBarrioFkNavigation.IdCiudadFkNavigation.DescripcionCiudad)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt style="display:none">
            @Html.DisplayNameFor(model => model.IdInstitucionFkNavigation)
        </dt>
        <dd style="display:none">
            @Html.DisplayFor(model => model.IdInstitucionFkNavigation.DescripcionInstitucion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdEdificio" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Vovler Atras</a>
</div>
