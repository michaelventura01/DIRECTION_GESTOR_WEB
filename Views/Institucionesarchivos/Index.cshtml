@model IEnumerable<DIRECTION_GESTOR.Models.Institucionesarchivos>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EnlaceArchivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaArchivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hora)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoArchivoFk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstadoFk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdInstitucionFkNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnlaceArchivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaArchivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoArchivoFk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstadoFk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInstitucionFkNavigation.DescripcionInstitucion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdArchivo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdArchivo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdArchivo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
