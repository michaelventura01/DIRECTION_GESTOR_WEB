@model DIRECTION_GESTOR.Models.Institucionestelefonos

@{
    ViewData["Title"] = "Create";
}

<partial name="../Telefonos/Create" />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdInstitucionFk" class="control-label"></label>
                <select asp-for="IdInstitucionFk" class="form-control" asp-items="ViewBag.IdInstitucionFk"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdTelefonoFk" class="control-label"></label>
                <select asp-for="IdTelefonoFk" class="form-control" asp-items="ViewBag.IdTelefonoFk"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdEstadoFk" class="control-label"></label>
                <select asp-for="IdEstadoFk" class="form-control" asp-items="ViewBag.IdEstadoFk"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


<partial name="../Institucionescorreos/Create" />
<partial name="../Institucionesarchivos/Create" />


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
