@model DIRECTION_GESTOR.Models.Movimientos

@{
    ViewData["Title"] = "Delete";
}
<br><br>
<h2>Eliminar</h2>
<br>
<h3>Deseas Eliminar esto?</h3>
<div>
    <h4>Movimientos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Descripcion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionMovimiento)
        </dd>
        <dt>
            Movimiento
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MontoMovimiento)
        </dd>

        <dt>
            Tipo de Movimiento
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdTipoMovimiento)
        </dd>
        <dt>
            Fecha
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaMovimiento)
        </dd>
        <dt>
            Hora
        </dt>
        <dd>
            @Html.DisplayFor(model => model.HoraMovimiento)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt style="display:none">
            @Html.DisplayNameFor(model => model.IdInstitucionFkNavigation)
        </dt>
        <dd style="display:none">
            @Html.DisplayFor(model => model.IdInstitucionFkNavigation.DescripcionInstitucion)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdMovimiento" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
