@model DIRECTION_GESTOR.Models.Paises

@{
    ViewData["Title"] = "Delete";
}
<br><br>
<h2>Eliminar</h2>

<h3>Deseas eliminar esto?</h3>
<div>
    <h4>Paises</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Pais
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionPais)
        </dd>
        <dt>
            Nacionalidad o Gentilicio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nacionalidad)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdPais" />
        <input type="submit" value="Delete" class="btn btn-primary" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
