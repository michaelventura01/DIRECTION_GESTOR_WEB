@model DIRECTION_GESTOR.Models.Paises

@{
    ViewData["Title"] = "Details";
}
<br><br>
<h2>Detalles</h2>
<br>
<div>
    <h4>Pais</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Pais
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionPais)
        </dd>
        <dt>
            Nacionalidad o Gentilicio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nacionalidad)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdPais" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
