@model DIRECTION_GESTOR.Models.Periodos

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>Quieres eliminar esto?</h3>
<div>
    <h4>Periodos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Periodo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionPeriodo)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdPeriodo" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
