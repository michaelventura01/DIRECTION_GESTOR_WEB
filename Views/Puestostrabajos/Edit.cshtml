@model DIRECTION_GESTOR.Models.Puestostrabajos

@{
    ViewData["Title"] = "Edit";
}
<br><br>
<h2>Modificar Puesto de Trabajo</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdPuestoTrabajo" />
            <div class="form-group">
                <label asp-for="DescripcionPuestoTrabajo" class="control-label">Puesto de Trabajo</label>
                <input asp-for="DescripcionPuestoTrabajo" class="form-control" />
                <span asp-validation-for="DescripcionPuestoTrabajo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Modificar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver Atras</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
