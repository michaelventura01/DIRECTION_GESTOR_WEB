@model DIRECTION_GESTOR.Models.Relaciones

@{
    ViewData["Title"] = "Details";
}
<br><br>
<h2>Detalles</h2>
<br>
<div>
    <h4>Relaciones</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Persona
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPersonaFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.IdPersonaFkNavigation.ApellidoPersonas)
        </dd>
        <dt>
            Relacionado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPersonaRelacionFkNavigation.NombrePersonas) @Html.DisplayFor(model => model.IdPersonaRelacionFkNavigation.ApellidoPersonas)
        </dd>
        <dt>
            Tipo de Relacion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdTipoRelacionFkNavigation.DescripcionTipoRelacion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdRelacion" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
