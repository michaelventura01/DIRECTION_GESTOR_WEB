@model IEnumerable<DIRECTION_GESTOR.Models.Responsabilidadobservaciones>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionResponsabilidadObservacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaResponsabilidadObservacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdInstitucionFk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstadoFkNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPersonaFkNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionResponsabilidadObservacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaResponsabilidadObservacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInstitucionFk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstadoFkNavigation.DescripcionEstado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPersonaFkNavigation.ApellidoPersonas)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdResponsabilidadObservacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdResponsabilidadObservacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdResponsabilidadObservacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
