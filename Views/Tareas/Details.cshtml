@model DIRECTION_GESTOR.Models.Tareas

@{
    ViewData["Title"] = "Details";
}
<br><br>
<h2>Detalles</h2>

<div>
    <h4>Tareas</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Titulo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TituloTarea)
        </dd>
        <dt>
            Descripcion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionTarea)
        </dd>
        <dt>
            Fecha
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FechaTarea)
        </dd>
        <dt>
            Hora
        </dt>
        <dd>
            @Html.DisplayFor(model => model.HoraTarea)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
        <dt style="display:none">
            @Html.DisplayNameFor(model => model.IdInstitucionFkNavigation)
        </dt>
        <dd style="display:none">
            @Html.DisplayFor(model => model.IdInstitucionFkNavigation.DescripcionInstitucion)
        </dd>
        <dt>
            Prioridad
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdPrioridadFkNavigation.DescripcionPrioridad)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdTarea" class="btn btn-primary">Modificar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
