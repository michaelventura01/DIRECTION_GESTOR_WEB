@model DIRECTION_GESTOR.Models.Tipocuentas

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>Deseas Eliminar Esto?</h3>
<div>
    <h4>Tipo de Cuenta</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Tipo de Cuenta
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionTipoCuenta)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdTipoCuenta" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
