@model DIRECTION_GESTOR.Models.Tipotelefonos

@{
    ViewData["Title"] = "Delete";
}
<br><br>
<h2>Eliminar</h2>
<hr>
<h3>Deseas eliminar esto?</h3>
<div>
    <h4>Tipo de Telefono</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Descripcion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DescripcionTipoTelefono)
        </dd>
        <dt>
            Estado
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdEstadoFkNavigation.DescripcionEstado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdTipoTelefono" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver Atras</a>
    </form>
</div>
